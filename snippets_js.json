    "body": [
      "function Main(input) {",
      "  let inp = input.split(\"\\n\");",
      "  // 整数の入力",
      "  let N = parseInt(inp[0]);",
      "  // 2つ以上の整数の入力",
      "  let [A, B] = inp[1].split(\" \");",
      "  A = Number(A);",
      "  B = Number(B);",
      "  // リストの入力",
      "  let L = inp[2].split(\" \").map(n => parseInt(n));",
      "  // 二次元配列の入力",
      "  let C = inp[3].map((n) => n.split(' ').map((n) => parseInt(n)));",
      "};",
      "",
      "Main(require(\"fs\").readFileSync(\"/dev/stdin\", \"utf8\"));"
    ],
    "description": "js入力と出力"
  },

